package helloPackage;

public class Car {
	
	String carname = "Nissan";
	String carmodel = "Altima";

	public String carName() {
		
		System.out.println(carname);
		System.out.println(carmodel);
		return carname;
	
	}
	
	public boolean isAvailable() {
		
		System.out.println("Car is Available");
		
		return true;
		
	}
	
	
	
}
